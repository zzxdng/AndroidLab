<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zinc.example.androidlab">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecyclerViewExampleActivity" />
        <activity android:name=".RecyclerViewFooterActivity" />
        <activity android:name=".expandablelistdemo.view.ExpandableListViewDemoActivity" />
        <activity android:name=".myexpandablelistview.MyExpandableListView" />
        <activity android:name=".rxjavademo.RxJavaDemoMainActivity" />
        <activity android:name=".viewpagerdemo.ViewPagerDemoActivity"></activity>
    </application>

</manifest>